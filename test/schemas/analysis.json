{
    "$id": "http://example.com/constants.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Constants",
    "type": "object",
    "required": ["data"],
    "properties": {
        "data": {
            "type": "object",
            "required": [
                "analysis"
            ],
            "properties": {
                "analysis": {
                    "type": "object",
                    "required": ["results"],
                    "properties": {
                        "results": {
                            "type": "object",
                            "required": ["bestMove"],
                            "properties": {
                                "bestMove": {
                                    "type": "object",
                                    "required": ["from", "to"],
                                    "properties": {
                                        "from": {
                                            "type": "string"
                                        },
                                        "to": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
